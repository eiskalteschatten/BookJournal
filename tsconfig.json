{
    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "node",
      "pretty": true,
      "sourceMap": true,
      "target": "es2020",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowJs": true,
      "outDir": "./dist",
      "baseUrl": "./src",
      "strictPropertyInitialization": false
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "dist"
    ]
  }
